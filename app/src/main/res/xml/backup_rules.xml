<?xml version="1.0" encoding="utf-8"?>
<!--
   Exclude ALL app data from auto-backup (API < 31).
   A Bitcoin wallet must never have its data backed up automatically.
-->
<full-backup-content>
    <exclude domain="root" path="." />
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="." />
</full-backup-content>